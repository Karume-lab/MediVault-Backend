{
  "name": "MediVault-Backend",
  "type": "module",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "dependencies": {
    "@dfinity/identity": "^1.0.1",
    "@dfinity/identity-secp256k1": "^1.0.1",
    "@types/cors": "^2.8.17",
